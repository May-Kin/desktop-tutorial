<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
	<SelectDialog
		id="selectDialog2"
		title="Employees"
		items="{/Employees}"
		search=".onValueHelpSearch2"
		confirm=".onValueHelpClose2"
		cancel=".onValueHelpClose2">
		<StandardListItem
			iconDensityAware="false"
			iconInset="false"
			title="{City}"
			description="{Address}"
			/>

	</SelectDialog>
</core:FragmentDefinition>